# MongoDB Cloud Connection (MongoDB Atlas)
MONGODB_URI=mongodb+srv://username:password@cluster-name.mongodb.net/travelbharat?retryWrites=true&w=majority

# Server Port (for deployment, typically 3000 or 5000)
PORT=5000

# JWT Secret (change this to a strong random string)
JWT_SECRET=your_jwt_secret_here_change_this_in_production

# Environment (production or development)
NODE_ENV=production

# Admin credentials (change these in production)
ADMIN_EMAIL=admin@travelBharat.com
ADMIN_PASSWORD=change_this_secure_password_in_production

# Frontend URL (for CORS configuration)
FRONTEND_URL=https://yourdomain.com
